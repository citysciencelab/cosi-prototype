{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "allowJs": true,
    "types": [
      "openlayers"
    ]
  },
  "include": [
    "../src/*",
    "../node_modules/angular-dashboard-components/components/charts/*/*",
    "../node_modules/angular-dashboard-components/components/enum/*",
    "../node_modules/angular-dashboard-components/components/utils/*"
  ],
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
